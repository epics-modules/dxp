# This database is used with mca.db for drivers that can issue callbacks when acquisition completes
record(ai,"$(P)$(M)ReadCallback") {
       field(SCAN,"I/O Intr")
       field(DTYP, "asynInt32")
       field(INP, "$(INP)ACQUIRING")
       field(FLNK, "$(P)$(M)ReadOnce PP MS")
}

record(bo,"$(P)$(M)ReadOnce") {
       field(VAL, "1")
       field(OUT, "$(P)$(M).READ PP MS")
}

