    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))DxpSCA$(SCA)Low")
    field(DESC, "SCA $(SCA) low channel")
    field(SCAN, "I/O Intr")
}

