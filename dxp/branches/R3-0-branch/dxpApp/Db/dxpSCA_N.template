record(longout, "$(P)$(R)SCA$(SCA)Low") {
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))DxpSCA$(SCA)Low")
    field(DESC, "SCA $(SCA) low channel")
    field(VAL,  "0")
}

